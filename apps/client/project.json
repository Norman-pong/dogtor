{
  "name": "@dogtor/client",
  "sourceRoot": "apps/client/src",
  "projectType": "application",
  "tags": ["type:app", "scope:client"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/client",
        "command": "bun run dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/client",
        "command": "bun run build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/client",
        "command": "bun run preview"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/client",
        "command": "bun run test"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/client",
        "command": "bun run typecheck"
      }
    }
  }
}