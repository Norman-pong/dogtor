{
  "name": "@dogtor/client",
  "sourceRoot": "apps/client/src",
  "projectType": "application",
  "tags": ["type:app", "scope:client"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @dogtor/client dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @dogtor/client build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @dogtor/client preview"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @dogtor/client test"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @dogtor/client typecheck"
      }
    }
  }
}