{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@dogtor/docs-site",
  "projectType": "application",
  "sourceRoot": "apps/docs-site/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs-site",
        "command": "bun run dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs-site",
        "command": "bun run build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/docs-site",
        "command": "bun run preview"
      }
    }
  },
  "tags": []
}