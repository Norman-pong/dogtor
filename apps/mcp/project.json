{
  "name": "@dogtor/mcp",
  "sourceRoot": "apps/mcp/src",
  "projectType": "application",
  "tags": [
    "type:app",
    "scope:mcp"
  ],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/mcp",
        "command": "bun run dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/mcp",
        "command": "bun run build"
      }
    },
    "server": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/mcp",
        "command": "bun run inspect:dev"
      }
    }
  }
}